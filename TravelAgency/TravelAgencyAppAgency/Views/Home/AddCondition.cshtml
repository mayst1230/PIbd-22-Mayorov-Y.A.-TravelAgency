@{ ViewData["Title"] = "Create"; }

<h1>Пополнение склада</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <label class="control-label">Склад</label>
                <select name="AgencyId" class="form-control" asp-items="@(new SelectList(ViewBag.Agencies, "Id", "AgencyName"))"></select>
            </div>
            <div class="form-group">
                <label class="control-label">Условие</label>
                <select name="ConditionId" class="form-control" asp-items="@(new SelectList(ViewBag.Conditions, "Id", "ConditionName"))"></select>
            </div>
            <div class="form-group">
                <label class="control-label">Количество</label>
                <input type="text" class="form-control" name="Count" />
            </div>
            <div class="form-group">
                <input type="submit" value="Пополнить" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
